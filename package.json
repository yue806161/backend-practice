{
  "name": "operate-database",
  "version": "1.0.0",
  "description": "",
  "main": "index",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "tsc",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "standard-version": {
    "types": [
      { "type": "feat", "section": "Added", "hidden": false },
      { "type": "fix", "section": "Fixed", "hidden": false },
      { "type": "chore", "section": "Chores", "hidden": false },
      { "type": "refactor", "section": "Changed", "hidden": false },
      { "type": "docs", "section": "Documentation", "hidden": false },
      { "type": "test", "section": "Tests", "hidden": false },
      { "type": "perf", "section": "Performance Improvements", "hidden": false },
      { "type": "style", "section": "Code Style", "hidden": false },
      { "type": "build", "section": "Build System", "hidden": false },
      { "type": "ci", "section": "Continuous Integration", "hidden": false }
    ],
    "releaseCommitMessageFormat": "chore(release): {{currentTag}}\n\n{{changelog}}",
    "infile": "docs/CHANGELOG.md",
    "header": "# Changelog\n\nAll notable changes to this project will be documented in this file.\n",
    "commitUrlFormat": "https://github.com/your-repo/commit/{{hash}}",
    "compareUrlFormat": "https://github.com/your-repo/compare/{{previousTag}}...{{currentTag}}",
    "issueUrlFormat": "https://github.com/your-repo/issues/{{id}}",
    "skip": {
      "tag": false
    }
  },
  "keywords": [],
  "author": "alex",
  "license": "MIT",
